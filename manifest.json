{
    "manifest_version": 2,
    "name": "Website Tracker",
    "description": "Tracks All Current Tab",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
    
    
}